@model IEnumerable<Example18.Models.Student>

@{
    ViewData["Title"] = "Students";
}


<h2>All Students</h2>

<table class="table">
    <tr>
        <th>
            First Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            Score
        </th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @item.FirstName
        </td>
        <td>
            @item.LastName
        </td>
        <td>
            @item.Score
        </td>
    </tr>
}
</table>
